{
	"name": "dataflow load title data to synapse",
	"properties": {
		"folder": {
			"name": "MappingDataFlow"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_TitleMaster",
						"type": "DatasetReference"
					},
					"name": "sourcetitledata"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_NYC_Payroll_TITLE_MD",
						"type": "DatasetReference"
					},
					"name": "sinktitledata"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcetitledata",
				"sourcetitledata sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TitleCode,",
				"          TitleDescription",
				"     )) ~> sinktitledata"
			]
		}
	}
}